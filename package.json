{
  "name": "mustadio",
  "version": "1.0.0",
  "description": "A companion app for FFT Battleground",
  "main": "src/main.js",
  "bin": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "start:loader": "node src/loader/index.js",
    "start:loader:debug": "node --inspect src/loader/index.js",
    "start:debug": "node --inspect src/main.js",
    "test": "mocha test --recursive",
    "build": "npm test && npx eslint src test && ./scripts/buildClient.sh",
    "loadMapImages": "node src/script/map-thumbs-loader.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsdom": "^16.4.0",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "pg": "^7.18.2",
    "pug": "^2.0.4",
    "sequelize": "^5.22.3",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^4.2.0",
    "swagger-ui-express": "^4.1.4",
    "tmi.js": "^1.5.0",
    "yamljs": "^0.3.0"
  },
  "pkg": {
    "scripts": "src/*.js",
    "assets": [
      "src/views/*",
      "src/public/*"
    ]
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-mocha": "^6.3.0",
    "mocha": "^7.2.0",
    "mock-require": "^3.0.3"
  }
}
